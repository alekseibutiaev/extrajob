  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch baseapp",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/baseapp",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": "/opt/ogre-1.9/lib"
        }
      ],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
